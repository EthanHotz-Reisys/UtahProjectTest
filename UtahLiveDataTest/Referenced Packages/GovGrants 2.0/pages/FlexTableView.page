<apex:page id="flexTableViewPage"  controller="GNT.FlexTableViewCtrl" showHeader="false" sidebar="false"  standardStylesheets="false">
    
    <c:ExternalLibrary />

    <apex:composition template="GNT__PhaseTemplate">
        <apex:define name="body">
            <c:FlexTable flexTableHeader="{!$CurrentPage.parameters.flexTableHeader}" listParams="{!parameterMap1}" flexTableName="{!flexTableName}" tableId="flexTableRandomID" flexTableParameters="{!parameterMap}"/>
        </apex:define>
    </apex:composition>
    
    <script>
        setPageTitle('{!$CurrentPage.parameters.pageTitle}');
    </script>
</apex:page>