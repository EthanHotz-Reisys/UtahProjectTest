<apex:page cache="false" standardController="Grant__c" extensions="GrantViewCtrl" sidebar="false" showHeader="false" standardStylesheets="false" action="{!updateStatus}">
    
    <!-- Including JS and CSS resources -->
    <GNT:ExternalLibrary />
    <c:ExternalLibrary />
    <apex:includeScript value="{!URLFOR($Resource.TabsPaging)}" />   
    <apex:composition template="GNT__PageTemplate" rendered="String">
         <apex:define name="content"> 
            <GNT:FlexViewLayout templateName="{!pageTemplateName}" layoutType="View" recordId="{!recordId}" deleteURL="{!deleteURL}"
                flexTableParameters="{!keyValueMap}"/>
        </apex:define>
    </apex:composition>
    
</apex:page>