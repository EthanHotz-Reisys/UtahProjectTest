<apex:page showHeader="false" standardStylesheets="false">
  <head>
    <title>Home</title>
     <script>
           j$(document).ready(function(){   
              if(document.getElementById('collabFeedWrapperID') != undefined){
                  document.getElementById('collabFeedWrapperID').style.height = '464px';  
              }
              j$(".type").click(function(){
              j$(".type").removeClass("active");
               j$(this).addClass("active");
            });
        });   
    </script>
