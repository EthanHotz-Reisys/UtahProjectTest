    <rules>
        <fullName>Award Rejection by sub</fullName>
        <actions>
            <name>Email_to_owner_when_sub_recipient_declines_the_award</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Award__c.Status__c</field>
            <operation>equals</operation>
            <value>Rejected</value>
        </criteriaItems>
        <description>Triggers email to owner when sub recipient declines the award.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
