    <rules>
        <fullName>EmailReminderforQ1</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Award__c.Status__c</field>
            <operation>contains</operation>
            <value>Created,Submitted for Approval,Approved,Activated,Being Amended,Amended,Rejected,Sent to Grantee Review,Grantee Accepted</value>
        </criteriaItems>
        <criteriaItems>
            <field>Award__c.GrantorOrganization__c</field>
            <operation>notEqual</operation>
            <value>USBE</value>
        </criteriaItems>
        <description>To send email to the subrecipient to remind about filling the Quarter details</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>EmailQ</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Award__c.Q1DueDate__c</offsetFromField>
            <timeLength>-5</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>EmailReminderforQ2</fullName>
