   private void programCodeUniquenessCheck(List<r_prog__c> newFinetPrograms){
    List<r_prog__c> ExistingFinetPrograms = [SELECT id,PROG_CD__c from r_prog__c];
    List<String> ExistingProgramCodes = new List<String> ();
    for (r_prog__c finetProg : ExistingFinetPrograms){
        ExistingProgramCodes.add(finetProg.PROG_CD__c);
    }

    for (r_prog__c finetProg : newFinetPrograms){
        if (ExistingProgramCodes.contains(finetProg.PROG_CD__c)){
            finetProg.addError('Program code already exists.');
        }
    }

   }
   
