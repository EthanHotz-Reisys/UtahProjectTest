    <fields>
        <fullName>HideEdit__c</fullName>
        <externalId>false</externalId>
        <formula>OR(
   NOT(
       OR(
          ISPICKVAL(PaymentRequest__r.Status__c, &apos;Created&apos;),
          ISPICKVAL(PaymentRequest__r.Status__c, &apos;Change Requested&apos;)
       )
   ),
   OR(CONTAINS(RowNumber__c, &apos;599&apos;),
      CONTAINS(RowNumber__c, &apos;213&apos;)
   ),
   AND($Profile.Name != &apos;SPI&apos;,
       $Profile.Name != &apos;SPA&apos;
   )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>HideEdit</label>
        <trackHistory>false</trackHistory>
