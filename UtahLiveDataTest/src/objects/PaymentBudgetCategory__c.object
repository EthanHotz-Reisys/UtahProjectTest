        <formula>OR(ISPICKVAL(PaymentRequest__r.Status__c,&apos;Approved&apos;),ISPICKVAL(PaymentRequest__r.Status__c,&apos;Paid&apos;),ISPICKVAL(PaymentRequest__r.Status__c,&apos;Rejected&apos;),ISPICKVAL(PaymentRequest__r.Status__c,&apos;Submitted to Grantor&apos;))</formula>
