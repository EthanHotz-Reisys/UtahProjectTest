    <validationRules>
        <fullName>Anticipated_Award_Date_Validation</fullName>
        <active>true</active>
        <description>Anticipated award date should be between project start date and application due date</description>
        <errorConditionFormula>AND(OR(DATETIMEVALUE(ApplicationDeadlineWithDateTime__c)&gt; DATETIMEVALUE(AnticipatedAwardDate__c), DATETIMEVALUE(EstimatedProjectPeriodStartDate__c) &lt; DATETIMEVALUE(AnticipatedAwardDate__c)), RecordType.DeveloperName = &apos;Grantee&apos;, OR(ISPICKVAL(Status__c, &apos;Created&apos;), ISPICKVAL(Status__c, &apos;Reviewed&apos;)))</errorConditionFormula>
        <errorMessage>Anticipated award date should be between  application due date and project start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Applicant_Type_Other_value</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISBLANK( OtherEligibleApplicantTypes__c ),
Includes(EligibleApplicantTypes__c,&quot;Other&quot;))</errorConditionFormula>
        <errorMessage>Eligible Applicant Type: Please enter other value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Application_Deadline_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>DATETIMEVALUE(ApplicationDeadlineWithDateTime__c)&gt; DATETIMEVALUE(EstimatedProjectPeriodStartDate__c)</errorConditionFormula>
        <errorMessage>Application Deadline  Date should not be later than Estimated Project Period Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AwardCeilingandCommittedAmountValidation</fullName>
        <active>false</active>
        <errorConditionFormula>AwardCeiling__c &gt; SetAsideAmount__c</errorConditionFormula>
        <errorDisplayField>AdditionalInformation__c</errorDisplayField>
        <errorMessage>Award Ceiling Amount can not be more than Committed Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Awarding_Federal_Agency_With_Other_Value</fullName>
        <active>false</active>
        <description>If Awarding Federal Agency values is other and other field is empty then throw an exception.</description>
        <errorConditionFormula>AND(
ISBLANK( IfOtherPleaseSpecify__c ),
Includes(AwardingFederalAgencyDOH__c,&quot;Other&quot;))</errorConditionFormula>
        <errorMessage>Awarding Federal Agency: Please enter other value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Budget_Period_End_Date_validation</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedBudgetPeriodEndDate__c &gt;  EstimatedProjectPeriodEndDate__c</errorConditionFormula>
        <errorMessage>Estimated Budget period End Date should not be later than Estimated Project Period End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Application_Deadline_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>FOAPublicationDate__c&gt;DATEVALUE(ApplicationDeadlineWithDateTime__c)</errorConditionFormula>
        <errorMessage>Application Deadline should be greater than FOA Publication date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Award_Ceiling_greater_than_floor</fullName>
        <active>true</active>
        <description>Check Award Ceiling greater than floor</description>
        <errorConditionFormula>AwardFloor__c &gt; AwardCeiling__c</errorConditionFormula>
        <errorMessage>Award Ceiling value should be greater than Award Floor.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Budget_Period_End_date</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedBudgetPeriodEndDate__c &lt;  EstimatedProjectPeriodStartDate__c</errorConditionFormula>
        <errorDisplayField>EstimatedBudgetPeriodEndDate__c</errorDisplayField>
        <errorMessage>Estimated Budget period End Date should not be earlier than Estimated Project Period Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Budget_Period_Start_date</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedBudgetPeriodStartDate__c &lt;  EstimatedProjectPeriodStartDate__c</errorConditionFormula>
        <errorMessage>Estimated Budget period Start Date should not be earlier than Estimated Project Period Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Budget_Period_Start_date2</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedBudgetPeriodStartDate__c &gt; EstimatedProjectPeriodEndDate__c</errorConditionFormula>
        <errorDisplayField>EstimatedBudgetPeriodStartDate__c</errorDisplayField>
        <errorMessage>Estimated Budget period Start Date should not be later than Estimated Project Period End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_CommittedAmount</fullName>
        <active>true</active>
        <description>Check SetAside Amount is less than CommittedAmount</description>
        <errorConditionFormula>AND(ObligatedAmount__c &gt; SetAsideAmount__c, ISPICKVAL(Status__c, &apos;Submitted for Approval&apos;))</errorConditionFormula>
        <errorMessage>Set Aside Amount can not be more than Committed Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Extension_Days</fullName>
        <active>true</active>
        <description>Check extension days when extension allowed is checked</description>
        <errorConditionFormula>IF(TimeExtensionAllowed__c, ExtensionDays__c == null, false)</errorConditionFormula>
        <errorMessage>Extension Days must have value if Extension Allowed is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_FOAPublication_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  RecordType.DeveloperName =&apos;Grantor&apos;  ,
FOAPublicationDate__c &lt; TODAY(), 
Text(Status__c)!=&apos;Published&apos;,
Text(Status__c)!=&apos;Closed&apos;,
Text(Status__c)!=&apos;Active&apos;,
Text(Status__c)!=&apos;RFA Published&apos;
)</errorConditionFormula>
        <errorMessage>FOA Release Date should be greater than or equal to Today&apos;s Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Current_Date_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED(FOAPublicationDate__c), FOAPublicationDate__c  &lt;  TODAY())</errorConditionFormula>
        <errorDisplayField>AdministrationBureauUnit__c</errorDisplayField>
        <errorMessage>FOA publication date should be greater than current date. Please check all the date field values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>IF ( PreApplicationRequired__c = false &amp;&amp; NOT(
 ISBLANK (PreApplicationDeadline__c)), true, false)</errorConditionFormula>
        <errorMessage>The PreApplication Deadline has to be empty if PreApplication Required is Unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Validation_2</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( PreApplicationRequired__c = true &amp;&amp; 
 (ISBLANK (PreApplicationDeadline__c))&amp;&amp; RecordType.Name = &apos;Grantor&apos;, true, false)</errorConditionFormula>
        <errorDisplayField>PreApplicationDeadline__c</errorDisplayField>
        <errorMessage>The PreApplication Deadline cannot be empty  if PreApplication Required is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Value_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( PreApplicationRequired__c = true &amp;&amp; ( (PostedDate__c &gt; PreApplicationDeadline__c)||( PreApplicationDeadline__c &gt;=  CloseDate__c)), true, false)</errorConditionFormula>
        <errorDisplayField>PreApplicationDeadline__c</errorDisplayField>
        <errorMessage>The PreApplication Deadline has to be less than the Close date and Greater than the Posted Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EnterEstimatedFundingAmtJustification</fullName>
        <active>true</active>
        <errorConditionFormula>AND( TEXT (AnnouncementType__c ) == &apos;Sole Source&apos;,
ISBLANK(  EstimatedFundingAmountJustification__c ),
( EstimatedFundingAmount__c &gt; 49999))</errorConditionFormula>
        <errorMessage>Available Funding Amount Justification: Please enter comments as the Available Funding Amount exceeds $49,999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EstimatedFundedAmount</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR(AwardCeiling__c != null,AwardFloor__c != null,EstimatedFundedAmount__c!=null,	EstimatedFundedAmount__c &gt;=AwardCeiling__c ,EstimatedFundedAmount__c &lt;= AwardFloor__c),true,false  
)</errorConditionFormula>
        <errorMessage>Estimated Request Amount  should be greater than Award floor and less than Award ceiling amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EstimatedProjectPeriodEndDate</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedProjectPeriodEndDate__c &lt;= EstimatedProjectPeriodStartDate__c</errorConditionFormula>
        <errorMessage>Estimated Project Period End Date cannot be less than or equal to the Estimated Project Period Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EstimatedProjectPeriodStartDate</fullName>
        <active>false</active>
        <errorConditionFormula>AND(EstimatedProjectPeriodStartDate__c &lt; today(), OR(ISPICKVAL(Status__c, &apos;Created&apos;), ISPICKVAL(Status__c, &apos;Reviewed&apos;)))</errorConditionFormula>
        <errorMessage>Estimated Project Period Start Date cannot be less than today&apos;s date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Estimated_Budget_date_validation</fullName>
        <active>false</active>
        <errorConditionFormula>EstimatedBudgetPeriodEndDate__c &lt; EstimatedBudgetPeriodStartDate__c</errorConditionFormula>
        <errorMessage>Estimated Budget Period End date should be greater than Estimated Budget Period Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Estimated_Project_date_validation</fullName>
        <active>true</active>
        <errorConditionFormula>EstimatedProjectPeriodEndDate__c &lt; EstimatedProjectPeriodStartDate__c</errorConditionFormula>
        <errorMessage>Estimated Project Period End date should be greater than Estimated Project Period Start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Extension_Day_Validation</fullName>
        <active>true</active>
        <description>Extension days must have value if Time Extension Allowed is checked</description>
        <errorConditionFormula>IF(TimeExtensionAllowed__c = true, ISBLANK(ExtensionDays__c), false)</errorConditionFormula>
        <errorMessage>Extension Days must have value if Time Extension Allowed is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Extension_Days_Validation</fullName>
        <active>true</active>
        <description>Extension Days should be a positive number</description>
        <errorConditionFormula>ExtensionDays__c &lt; 0</errorConditionFormula>
        <errorMessage>Extension Days should be a positive number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinalApproverNotRequired</fullName>
        <active>false</active>
        <description>If Pre-Application checkbox is Unchecked, Final Approver should be empty.</description>
        <errorConditionFormula>IF(PreApplicationRequired__c = false &amp;&amp; 
 (LEN (PreApplicationFinalApprover__c)&gt; 0), true, false)</errorConditionFormula>
        <errorDisplayField>PreApplicationFinalApprover__c</errorDisplayField>
        <errorMessage>If Pre-Application checkbox is Unchecked, Final Approver should be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FinalApproverRequired</fullName>
        <active>false</active>
        <description>If Pre-Application checkbox is checked, Final Approver is Required.</description>
        <errorConditionFormula>IF(PreApplicationRequired__c == true &amp;&amp; PreApplicationFinalApprover__c == null &amp;&amp; RecordType.Name = &apos;Grantor&apos;, true, false)</errorConditionFormula>
        <errorMessage>If Pre-Application checkbox is checked, Final Approver is Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Post_Date_before_Close_Date</fullName>
        <active>true</active>
        <description>The positing date cannot be after the closing date</description>
        <errorConditionFormula>PostedDate__c  &gt; CloseDate__c</errorConditionFormula>
        <errorDisplayField>CloseDate__c</errorDisplayField>
        <errorMessage>The Close Date must be after the Posted Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreApplication_Deadline_Check</fullName>
        <active>true</active>
        <description>Pre Application Deadline can not be after Application Deadline</description>
        <errorConditionFormula>PreApplicationDeadlineWithDateTime__c&gt;ApplicationDeadlineWithDateTime__c</errorConditionFormula>
        <errorMessage>Pre Application Deadline can not be after Application Deadline</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RFA_Release_Date_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RFARequired__c,FOAPublicationDate__c &gt; RFAReleaseDate__c)</errorConditionFormula>
        <errorDisplayField>RFAReleaseDate__c</errorDisplayField>
        <errorMessage>RFA Release date should be later than FOA Publication date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Recipient_Match_Validation</fullName>
        <active>true</active>
        <description>Sub_Recipient_Match_Validation</description>
        <errorConditionFormula>AND(MatchRequired__c, ISNULL(GranteeMatch__c) )</errorConditionFormula>
        <errorMessage>Sub Recipient Match % needs to be entered</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
