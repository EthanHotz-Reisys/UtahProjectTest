    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>Award_Issue_Date_Validation</fullName>
        <active>true</active>
        <description>Award issue date should be a future date</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;Created&apos;), DATEVALUE(AwardIssueDate__c) &lt; TODAY())</errorConditionFormula>
        <errorMessage>Award issue date should be a future date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BudgetEndGreaterThanProjectEnd</fullName>
        <active>true</active>
        <errorConditionFormula>BudgetPeriodEndDate__c &gt; ProjectPeriodEndDate__c</errorConditionFormula>
        <errorMessage>The budget period end date cannot occur before the project period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BudgetStartGreaterThanEnd</fullName>
        <active>true</active>
        <errorConditionFormula>BudgetPeriodStartDate__c &gt; BudgetPeriodEndDate__c</errorConditionFormula>
        <errorMessage>The budget period start date should be earlier than the budget period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BudgetStartGreaterThanProjectEnd</fullName>
        <active>true</active>
        <errorConditionFormula>BudgetPeriodStartDate__c &gt; ProjectPeriodEndDate__c</errorConditionFormula>
        <errorMessage>The budget period start date should be earlier than the project period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContinuationStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  AND(
    NOT(ISNULL(PreviousBudgetEndDate__c)),
    NOT(ISNULL(BudgetPeriodStartDate__c)),
    CONTAINS(Amendments__c, &apos;Continuation&apos;)
  ),
  IF(
    PreviousBudgetEndDate__c &gt; BudgetPeriodStartDate__c,
    true,
    false
  ),
  false
)</errorConditionFormula>
        <errorMessage>The new budget period start date must occur after the previous budget period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IssueAwardAmountNotGreaterObligatedAmoun</fullName>
        <active>false</active>
        <description>Award Amount This Action cannot be greater than Obligated Amount.</description>
        <errorConditionFormula>AwardedAmountthisAction__c  &gt;=  TotalObligateAmount__c</errorConditionFormula>
        <errorDisplayField>AwardedAmountthisAction__c</errorDisplayField>
        <errorMessage>Award Amount this Action cannot be greater than Total Obligated Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProjectStartGreaterThanBudgetEnd</fullName>
        <active>true</active>
        <errorConditionFormula>ProjectPeriodStartDate__c &gt; BudgetPeriodEndDate__c</errorConditionFormula>
        <errorMessage>The project period start date should be earlier than budget period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProjectStartGreaterThanBudgetStart</fullName>
        <active>true</active>
        <errorConditionFormula>ProjectPeriodStartDate__c &gt; BudgetPeriodStartDate__c</errorConditionFormula>
        <errorMessage>The project period start date should be earlier than or equal to the budget period start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProjectStartGreaterThanEnd</fullName>
        <active>true</active>
        <errorConditionFormula>ProjectPeriodStartDate__c &gt; ProjectPeriodEndDate__c</errorConditionFormula>
        <errorMessage>The project period start date should be earlier than the project period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Greater_Than_ISSUE_DATE</fullName>
        <active>false</active>
        <description>The project start date should be greater than the award issue date</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &apos;Created&apos;), ProjectPeriodStartDate__c &lt; DATEVALUE(AwardIssueDate__c))</errorConditionFormula>
        <errorMessage>The project start date should be greater than the award issue date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TestMSPValidation</fullName>
        <active>false</active>
        <errorConditionFormula>Grant__r.ObjectCode__c = &apos;461&apos;</errorConditionFormula>
        <errorMessage>Error 461</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidationonIsStateMatchRequired</fullName>
        <active>false</active>
        <description>If Is state match Required is True, Enter State match Due Date and State Share Amount</description>
        <errorConditionFormula>AND(IsStateMatchRequired__c = true, ISBLANK( StateMatchDueDate__c ), ISBLANK( StateShareAmount__c ) )</errorConditionFormula>
        <errorMessage>Please Enter &apos;State Match Due date&apos;,&apos;State Share Amount&apos; if &apos;Is State Match Required?&apos; is Checked.</errorMessage>
    </validationRules>
