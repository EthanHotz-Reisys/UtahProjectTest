    <fields>
        <fullName>NoOfMonthsOfBudgetPeriod__c</fullName>
        <description>This field is used to calculate the number of a months in a budget period. It is used in reports</description>
        <externalId>false</externalId>
        <formula>IF( AND(
          NOT( ISBLANK(BudgetPeriod__r.BudgetStartDate__c ) ) ,
          NOT( ISBLANK(BudgetPeriod__r.BudgetEndDate__c) ) 
         ) , 
    IF( YEAR(BudgetPeriod__r.BudgetStartDate__c) =  YEAR(BudgetPeriod__r.BudgetEndDate__c)  , 
         IF( MONTH(BudgetPeriod__r.BudgetStartDate__c) =  MONTH(BudgetPeriod__r.BudgetEndDate__c), 
            1, 
            ABS (MONTH(BudgetPeriod__r.BudgetEndDate__c) -  MONTH(BudgetPeriod__r.BudgetStartDate__c))), 
         (((YEAR(BudgetPeriod__r.BudgetEndDate__c) -  YEAR(BudgetPeriod__r.BudgetStartDate__c)) *       12) - ( ABS( MONTH(BudgetPeriod__r.BudgetEndDate__c) -  MONTH(BudgetPeriod__r.BudgetStartDate__c)  )))),                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         0)</formula>
        <label>No Of Months Of Budget Period</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
