        <externalId>false</externalId>
        <formula>AND(
  OR(
    AND(
      NOT(ISPICKVAL(Status__c, &apos;Drafted&apos;))
    ),
    AND(
      $User.Id != GrantMonitor__c,
      $User.Id != ProgramManager__c,
      $User.Id != ProjectOfficer__c,
      $User.Id != DataEvaluator__c,
      $User.Id != GrantSupervisor__c,
      $User.Id != BureauChief__c
    ),
    AND(
      NOT(ISPICKVAL(AwardType__c, &apos;New&apos;)),
      NOT(ISPICKVAL(AwardType__c, &apos;Continuation&apos;)),
      NOT(CONTAINS(Amendments__c, &apos;Award Increase/Decrease&apos;)),
      NOT(CONTAINS(Amendments__c, &apos;Cost-Extension&apos;))
    )
  ),
  OR( 
    NOT(ISPICKVAL(Status__c, &apos;Accepted by Sub-Awardee&apos;)), 
    $User.Id != GrantSupervisor__c 
  ),
  OR( 
    NOT(ISPICKVAL(Status__c, &apos;Sub-Awardee Review&apos;)), 
    $User.Id != SubAwardee__c 
  ),
  OR(
    NOT(ISPICKVAL(Status__c, &apos;Approved by OD&apos;)), 
    $Profile.Name != &apos;Executive User&apos;
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
