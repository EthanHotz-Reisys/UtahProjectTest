        <errorMessage>The total amount cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CashMatchBudgetLessThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>CashMatchRemaining__c&lt;0</errorConditionFormula>
        <errorMessage>The remaining cash match budget cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EditAfterSubmission</fullName>
        <active>true</active>
        <description>AND(
  ggsDemo__Award__r.ggsDemo__ReadOnly__c,
  NOT(AND(
    ISPICKVAL(ggsDemo__Award__r.ggsDemo__Status__c, &apos;Sub-Awardee Review&apos;),
    $Profile.Name = &apos;Project Admin&apos;
  ))
)</description>
        <errorConditionFormula>false</errorConditionFormula>
        <errorMessage>You cannot edit the budget of this award.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FederalRemainingLessThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>FederalRemaining__c&lt;0</errorConditionFormula>
        <errorMessage>The remaining budget cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InKindRemainingLessThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>InKindRemaining__c&lt;0</errorConditionFormula>
        <errorMessage>The remaining in kind budget cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RemainingBudgetLessThanZero</fullName>
        <active>true</active>
        <errorConditionFormula>RemainingBudget__c&lt;0</errorConditionFormula>
        <errorMessage>The remaining budget cannot be less than zero.</errorMessage>
    </validationRules>
