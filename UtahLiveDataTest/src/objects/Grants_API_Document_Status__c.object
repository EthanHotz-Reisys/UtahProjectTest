        <fullName>Grants_API_Document_Final__c</fullName>
        <description>Flag to show if the document is in a final state.</description>
        <externalId>false</externalId>
        <formula>UPPER( TEXT( Grants_API_Document_Status__c ) ) == &quot;REJECTED&quot; ||
UPPER( TEXT( Grants_API_Document_Phase__c ) ) == &quot;FINAL&quot; ||
(Count_Of_Children__c &gt; 0 &amp;&amp; Count_Of_Children__c  ==  Count_Of_Inactive_Children__c)</formula>
