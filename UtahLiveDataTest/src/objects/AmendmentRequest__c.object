    <validationRules>
        <fullName>AtleastOneChangeisNeeded</fullName>
        <active>false</active>
        <description>Please check any of these fields:Cost Increase,Grant award extension, Cost Increase, Scope of Work Change, Key Contact Change and Other Change for Amendment Preparation.</description>
        <errorConditionFormula>AND(GrantAwardExtension__c = False,  CostIncrease__c = False,  ScopeofWorkChange__c = False,  KeyContactChange__c = False,  OtherChange__c = False, TEXT(Status__c) = &apos;Created&apos;)</errorConditionFormula>
        <errorMessage>Please check any of these fields:Cost Increase,Grant award extension, Cost Increase, Scope of Work Change, Key Contact Change and Other Change for Amendment Preparation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfCostIncreaseEnterIncreaseAmount</fullName>
        <active>true</active>
        <description>If Cost Increase is checked, enter Cost Increase Amount.</description>
        <errorConditionFormula>AND(  CostIncrease__c = TRUE, ISBLANK(  Requested_Change_In_Budget__c ) )</errorConditionFormula>
        <errorMessage>Please Enter Cost Increase Amount if Cost Increase is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfGrantAwardExtensionEnterNewEndDate</fullName>
        <active>true</active>
        <description>If Grant Award Extension is checked, please Enter New End Date</description>
        <errorConditionFormula>AND(  GrantAwardExtension__c = TRUE, ISBLANK(  NewEndDate__c ) )</errorConditionFormula>
        <errorMessage>Please Enter New End Date if Grant Award Extension is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfOtherChangeEnterOtherDescription</fullName>
        <active>true</active>
        <description>If &apos;Other Change&apos; is checked, please enter &apos;Other Change Description&apos;.</description>
        <errorConditionFormula>AND(  OtherChange__c = TRUE, ISBLANK(  OtherChangeDescription__c ) )</errorConditionFormula>
        <errorMessage>Please enter &apos;Other Change Description&apos;, if &apos;Other Change&apos; is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IfScopeOfWorkChangeEnterNewScope</fullName>
        <active>true</active>
        <description>If &apos;Scope of Work Change&apos; is checked, enter new scope of work.</description>
        <errorConditionFormula>AND(  ScopeofWorkChange__c = TRUE, ISBLANK(  ScopeofWorkChangeDescription__c ) )</errorConditionFormula>
        <errorMessage>Please Enter New Scope ,if Scope of Work Change is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NewBudgetPeriodEndDateIsEmpty</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NoCostExtension__c==TRUE, ISBLANK(NewBudgetPeriodEndDate__c))</errorConditionFormula>
        <errorMessage>Please enter new budget period end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoCostExtensionBudgetCategoryChange</fullName>
        <active>true</active>
        <description>User cannot change both No Cost Extention and Change Budget Categories</description>
        <errorConditionFormula>AND(NoCostExtension__c==TRUE ,BudgetPeriodExtension__c==TRUE)</errorConditionFormula>
        <errorMessage>No cost extension and Budget categories cannot be changed at the same time. Please select either of one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequestTypeRequired</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(RequestType__c, &quot;&quot; )</errorConditionFormula>
        <errorMessage>Request Type is a required field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VerifyBudgetEndDate</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
  AND( 
    NOT(ISNULL(ProposedBudgetEndDate__c)), 
    NOT(ISNULL(ProposedProjectEndDate__c))
  ), 
  ProposedBudgetEndDate__c &gt; ProposedProjectEndDate__c,
  false
)</errorConditionFormula>
        <errorMessage>The budget period end date cannot be later than the project end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VerifyCostBudgetValidation</fullName>
        <active>true</active>
        <description>This validation rule validates that only either one among the following three are checked No Cost Extension, Cost Change Only and Cost and Budget Period Change. (API Names:
NoCostExtension__c,BudgetPeriodExtension__c,BudgetPeriodChange__c)</description>
        <errorConditionFormula>OR(
   AND(
      NoCostExtension__c=TRUE,
      BudgetPeriodExtension__c=TRUE,
      BudgetPeriodChange__c=TRUE 
     ),
   AND(
      NoCostExtension__c=TRUE,
      BudgetPeriodExtension__c=TRUE 
     ),
   AND(
      BudgetPeriodExtension__c=TRUE,
      BudgetPeriodChange__c=TRUE  
     ),
   AND(
      NoCostExtension__c=TRUE,
      BudgetPeriodChange__c=TRUE  
     )
)</errorConditionFormula>
        <errorMessage>Please check only ONE of the following: No Cost Extension or Cost Change Only or Cost and Budget Period Change</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VerifyEndDate2</fullName>
        <active>true</active>
        <description>The project period end date cannot occur before the project period end date.</description>
        <errorConditionFormula>IF(
  NOT(ISNULL(ProposedProjectEndDate__c)),
  ProposedProjectEndDate__c &lt; AwardedProjectPeriodEndDate__c, 
  false
)</errorConditionFormula>
        <errorMessage>The proposed project end date should be later than the current project period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VerifyEndDate3</fullName>
        <active>true</active>
        <errorConditionFormula>ProposedBudgetEndDate__c &lt; AwardedBudgetPeriodEndDate__c</errorConditionFormula>
        <errorMessage>The proposed budget period end date should be later than the current budget period end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VerifyImpact</fullName>
        <active>true</active>
        <description>This filed will let the reviewer know, to determine the impact in case if the Amendment Request is not approved.</description>
        <errorConditionFormula>ISBLANK( Impact__c )</errorConditionFormula>
        <errorMessage>Impact filed cannot be left blank</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
