    <validationRules>
        <fullName>Admin_Limit</fullName>
        <active>true</active>
        <errorConditionFormula>AdminRequestedExpense__c &gt; RequestedAmountthisAction__c * 0.05</errorConditionFormula>
        <errorMessage>Admin Requested Expense Can Not be Greater than 5% of Total Requested Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Date_Validation</fullName>
        <active>true</active>
        <description>Billing date validation for Advance</description>
        <errorConditionFormula>TEXT(Type__c) == &apos;Advance&apos; &amp;&amp;  (BillingStartDate__c &lt;  Award__r.ProjectPeriodStartDate__c || BillingStartDate__c &gt; Award__r.ProjectPeriodEndDate__c ||
BillingEndDate__c &lt;  Award__r.ProjectPeriodStartDate__c || BillingStartDate__c &gt; Award__r.ProjectPeriodEndDate__c )</errorConditionFormula>
        <errorMessage>Billing start Date/End Date should be within Project Start Date/End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Date_Validation2</fullName>
        <active>true</active>
        <description>Billing date validation for Advance</description>
        <errorConditionFormula>BillingStartDate__c &gt; BillingEndDate__c</errorConditionFormula>
        <errorMessage>Billing Start Date cannot be greater than Billing End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OverExpenditure</fullName>
        <active>false</active>
        <errorConditionFormula>RequestedAmountthisAction__c  &gt;  AwardedAmount__c</errorConditionFormula>
        <errorMessage>Requested Amount this Action cannot be greater than Awarded Amount</errorMessage>
    </validationRules>
