        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HideFlexActions__c</fullName>
        <externalId>false</externalId>
        <formula>IF((ISPICKVAL(SiteVisit__r.Status__c,&apos;Initiated By PO&apos;) || ISPICKVAL(SiteVisit__r.Status__c,&apos;Grant Monitor Rejected&apos;) || ISPICKVAL(SiteVisit__r.Status__c,&apos;Program Manager Rejected&apos;)) &amp;&amp;  ($UserRole.Name == &apos;CHA Project Officer&apos; || $UserRole.Name == &apos;HAHSTA Project Officer&apos;),false,true)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>HideFlexActions</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
