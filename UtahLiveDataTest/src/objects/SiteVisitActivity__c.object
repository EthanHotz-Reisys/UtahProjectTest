    <fields>
        <fullName>HideFindingsButton__c</fullName>
        <externalId>false</externalId>
        <formula>IF(($User.IsApplicant__c), NOT(ISPICKVAL(SiteVisit__r.Status__c,&apos;Sent to Subrecipient&apos;)), NOT(ISPICKVAL(SiteVisit__r.Status__c,&apos;Visited&apos;)))</formula>
