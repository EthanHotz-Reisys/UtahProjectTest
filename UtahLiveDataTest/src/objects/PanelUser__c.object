        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>ReviewerName__c</fullName>
        <externalId>false</externalId>
        <formula>Reviewer__r.FirstName + &quot; &quot; + Reviewer__r.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ReviewerName</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ReviewerProfile__c</fullName>
        <externalId>false</externalId>
        <formula>if(TRIM(Reviewer__r.Profile.Name)= &apos;PT1&apos; , &apos;System Administrator&apos;,Reviewer__r.Profile.Name)</formula>
        <label>Reviewer Profile</label>
        <required>false</required>
