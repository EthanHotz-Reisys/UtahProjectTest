    <fields>
        <fullName>FundStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF(RemainingBalance__c &lt; 0, &apos;&lt;i class=&quot;fa fa-thumbs-o-down fa-lg&quot; title=&quot;Deficit&quot;&gt;&lt;/i&gt;&apos;, &apos;&lt;i class=&quot;fa fa-thumbs-o-up fa-lg&quot; title=&quot;Ok&quot;&gt;&lt;/i&gt;&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Fund Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>FundingAccount__c</fullName>
        <externalId>false</externalId>
        <label>Funding Account</label>
        <referenceTo>FundingAccount__c</referenceTo>
        <relationshipLabel>Funding Opportunity Funding Accounts</relationshipLabel>
        <relationshipName>Announcement_Funding_Accounts</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>HideAnnouncementEdit__c</fullName>
        <externalId>false</externalId>
        <formula>NOT(OR(ISPICKVAL(Announcement__r.Status__c,&apos;Created&apos;), 
ISPICKVAL(Announcement__r.Status__c,&apos;Reviewed&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hide Announcement Edit</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HideRemoveFundingAccounts__c</fullName>
        <externalId>false</externalId>
        <formula>NOT(AND(Announcement__r.ScheduleManager__c = $User.Id, OR(text(Announcement__r.Status__c) = &apos;In Progress&apos; ,text(Announcement__r.Status__c) = &apos;Validated&apos;) ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>HideRemoveFundingAccounts</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
