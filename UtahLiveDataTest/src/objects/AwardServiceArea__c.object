    <fields>
        <fullName>HideEdit__c</fullName>
        <externalId>false</externalId>
        <formula>OR(
  AND(
    $User.Id != Award__r.ProjectOfficer__c,
    $User.Id != Award__r.GrantMonitor__c,
    $User.Id != Award__r.DataEvaluator__c,
    $User.Id != Award__r.ProgramManager__c
  ),
  NOT(ISPICKVAL(Award__r.Status__c, &apos;Drafted&apos;)),
  AND(
    NOT(CONTAINS(Award__r.Amendments__c,&apos;Award Increase/Decrease&apos;)),
    NOT(CONTAINS(Award__r.Amendments__c,&apos;Cost-Extension&apos;)),
    NOT(CONTAINS(Award__r.Amendments__c,&apos;Budget Revision&apos;)),
    NOT(ISBLANK(Award__r.Amendments__c))
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hide Edit</label>
        <trackHistory>false</trackHistory>
