    <fields>
        <fullName>HideAnnouncementEdit__c</fullName>
        <externalId>false</externalId>
        <formula>OR(NOT(ISPICKVAL(FundingOpportunity__r.Status__c,&apos;Created&apos;)), 
NOT(ISPICKVAL(FundingOpportunity__r.Status__c,&apos;Reviewed&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hide Announcement Edit</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HideApplicationEdit__c</fullName>
        <externalId>false</externalId>
        <formula>NOT(OR(ISPICKVAL(Application__r.Status__c,&apos;Created&apos;), ISPICKVAL(Application__r.Status__c,&apos;Internally Reviewed&apos;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hide Application Edit</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HideConvertBtn__c</fullName>
        <externalId>false</externalId>
        <formula>NotOwner__c || ISPICKVAL(EvaluationStatus__c, &apos;Converted to Opportunity&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>HideConvertBtn</label>
        <trackTrending>false</trackTrending>
