    </fields>
    <fields>
        <fullName>HideEditBtn__c</fullName>
        <description>Logic for Hiding Edit Button</description>
        <externalId>false</externalId>
        <formula>OR(AND(FundingOpportunity__c!=null,FundingOpportunity__r.HideEditBtn__c),
   AND(Award__c!=null,Award__r.HideNewBtn__c),
   AND(Application__c!=null,Application__r.RenderGranteeDelete__c)
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Logic for Hiding Edit Button</inlineHelpText>
        <label>Hide Edit Btn</label>
