    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>BeginningEndingDate</fullName>
        <active>false</active>
        <errorConditionFormula>IF(ISPICKVAL(ReportStartDate__c,&apos;Project Period Start Date&apos;) &amp;&amp; ISPICKVAL (ReportEndDate__c,&apos;Project Period End Date&apos;),false,true)</errorConditionFormula>
        <errorMessage>Beginning Date and Ending Date should both be From Project Period , Budget Period OR Fixed Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BudgetPeriodStartAndEndDate_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(  ISPICKVAL(ReportStartDate__c,&apos;Budget Period Start Date&apos;) &amp;&amp; ISPICKVAL(ReportEndDate__c,&apos;Budget Period End Date&apos;) &amp;&amp; (Award__r.BudgetPeriodStartDate__c == null || Award__r.BudgetPeriodEndDate__c == null),true,false)</errorConditionFormula>
        <errorMessage>Budget Period Start/End Date needs to be filled out on the award</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Availability_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISCHANGED( AvailabilityDate__c ) ,AvailabilityDate__c &lt; TODAY(),false)</errorConditionFormula>
        <errorDisplayField>AvailabilityDate__c</errorDisplayField>
        <errorMessage>Past date cannot be entered.Please enter a valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Effective_Till_Date</fullName>
        <active>true</active>
        <errorConditionFormula>EffectiveTill__c &lt; EffectiveFrom__c</errorConditionFormula>
        <errorDisplayField>EffectiveTill__c</errorDisplayField>
        <errorMessage>Effective Till Date can not be before Effective From Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Fixed_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISCHANGED( FixedEndDate__c ) ,FixedEndDate__c &lt; TODAY(),false)</errorConditionFormula>
        <errorDisplayField>FixedEndDate__c</errorDisplayField>
        <errorMessage>Past date cannot be entered.Please enter a valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Fixed_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISCHANGED( FixedStartDate__c) ,FixedStartDate__c &lt; TODAY(),false)</errorConditionFormula>
        <errorDisplayField>FixedStartDate__c</errorDisplayField>
        <errorMessage>Past date cannot be entered.Please enter a valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Inactive_on_Date</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISCHANGED( InactiveOn__c) ,InactiveOn__c &lt; TODAY(),false)</errorConditionFormula>
        <errorDisplayField>InactiveOn__c</errorDisplayField>
        <errorMessage>Past date cannot be entered.Please enter a valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Report_Submission_Date</fullName>
        <active>false</active>
        <errorConditionFormula>IF( ISCHANGED( UpcomingReportSubmissionDate__c) ,UpcomingReportSubmissionDate__c &lt; TODAY(),false)</errorConditionFormula>
        <errorDisplayField>UpcomingReportSubmissionDate__c</errorDisplayField>
        <errorMessage>Past date cannot be entered.Please enter a valid date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Validation_in_Site_Visit_Dates</fullName>
        <active>false</active>
        <description>Date Validation in Site Visit Dates</description>
        <errorConditionFormula>IF((RecordType.Name ==&apos;Site Visit&apos; ) &amp;&amp; OR(VisitEndDateTime__c &lt; SiteVisitDateTime__c,DateValue(VisitEndDateTime__c) &lt;  Award__r.BudgetPeriod__r.BudgetStartDate__c ,DateValue(VisitEndDateTime__c) &gt;  Award__r.BudgetPeriod__r.BudgetEndDate__c ),true,false)</errorConditionFormula>
        <errorMessage>Site Visit End Date can not be before Site Visit Start Date and it should be in between Budget Period Start Date and Budget Period End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Due_Date_Days_Cannot_be_Negative</fullName>
        <active>true</active>
        <description>Due Date Days Cannot be Negative</description>
        <errorConditionFormula>DueDateDays__c &lt; 0</errorConditionFormula>
        <errorDisplayField>DueDateDays__c</errorDisplayField>
        <errorMessage>Due Date Days cannot be negative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FixedDateCantbenullifFixedChosen</fullName>
        <active>true</active>
        <description>Fixed Date Cant be null if Fixed Chosen</description>
        <errorConditionFormula>IF(ISPICKVAL(ReportStartDate__c,&apos;Fixed&apos;) &amp;&amp; ISPICKVAL(ReportEndDate__c,&apos;Budget Period End Date&apos;) || ISPICKVAL(ReportStartDate__c,&apos;Budget Period Start Date&apos;) &amp;&amp; ISPICKVAL(ReportEndDate__c,&apos;Fixed&apos;),true,false)</errorConditionFormula>
        <errorMessage>Please enter the fixed Start and End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FixedDateValidation</fullName>
        <active>true</active>
        <errorConditionFormula>FixedStartDate__c &gt; FixedEndDate__c</errorConditionFormula>
        <errorMessage>Start date cannot be after end date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FixedDatesCantBeNullIfBothFixedChosen</fullName>
        <active>true</active>
        <errorConditionFormula>IF((ISPICKVAL(ReportStartDate__c,&apos;Fixed&apos;) &amp;&amp; ISPICKVAL(ReportEndDate__c,&apos;Fixed&apos;) )&amp;&amp; (ISBLANK(FixedStartDate__c) || ISBLANK(FixedEndDate__c)),true,false)</errorConditionFormula>
        <errorMessage>Please enter respective dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FixedDatesShouldbeWithinBudgetPeriod</fullName>
        <active>true</active>
        <errorConditionFormula>IF((ISPICKVAL(ReportStartDate__c,&apos;Fixed&apos;) &amp;&amp; ISPICKVAL(ReportEndDate__c,&apos;Fixed&apos;)) &amp;&amp; ((FixedStartDate__c &lt; Award__r.BudgetPeriodStartDate__c || FixedStartDate__c &gt; Award__r.BudgetPeriodEndDate__c) || (FixedEndDate__c &lt; Award__r.BudgetPeriodStartDate__c || FixedEndDate__c &gt; Award__r.BudgetPeriodEndDate__c )),true,false)</errorConditionFormula>
        <errorMessage>Fixed Dates have to be within Budget Period Start and End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventUpdationatGrantLevel</fullName>
        <active>false</active>
        <description>Prem - De activated this validation rule because we need to update T&amp;C in the batch job.
Let&apos;s chalk out some other way to handle this validation.</description>
        <errorConditionFormula>IF(AND(
NOT(ISNEW()),NOT(ISBLANK(PRIORVALUE(Grant__c))),ISBLANK( Award__c )
	), 
    TRUE, 
    AND(
		ISCHANGED(Grant__c), NOT(ISBLANK(PRIORVALUE(Grant__c)) )
	)
)</errorConditionFormula>
        <errorMessage>You cannot update a record associated with a grant.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReportingRequirementEndDateValidation</fullName>
        <active>false</active>
        <description>Reporting Requirement End Date should be in between Budget Period Start Date and Budget Period End date and also greater than Reporting Requirement Start Date.</description>
        <errorConditionFormula>IF(OR(RecordType.Name ==&apos;Reporting Requirement&apos;,RecordType.Name ==&apos;Payment Request&apos;) &amp;&amp; OR(CalReportingEndDate__c &lt; CalReportingStartDate__c ,	CalReportingEndDate__c &lt; Award__r.BudgetPeriod__r.BudgetStartDate__c ,	CalReportingEndDate__c &gt;  Award__r.BudgetPeriod__r.BudgetEndDate__c) ,true,false)</errorConditionFormula>
        <errorMessage>Reporting End Date Should be in between Budget Period Start date and Budget Period End Date  and also greater than Reporting Requirement Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReportingRequirementStartDateValidation</fullName>
        <active>false</active>
        <description>Reporting Requirement Start Date should be in between Budget Period Start Date and Budget Period End date.</description>
        <errorConditionFormula>IF(OR(RecordType.Name ==&apos;Reporting Requirement&apos;,RecordType.Name ==&apos;Payment Request&apos; ) &amp;&amp; OR(	CalReportingStartDate__c &lt; Award__r.BudgetPeriod__r.BudgetStartDate__c ,	CalReportingStartDate__c &gt;  Award__r.BudgetPeriod__r.BudgetEndDate__c) ,true,false)</errorConditionFormula>
        <errorMessage>Reporting Start Date Should be in between Budget Period Start date and Budget Period End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_either_Program_or_Award_or_Grant</fullName>
        <active>false</active>
        <errorConditionFormula>IF( (Award__c == NULL &amp;&amp;  Grant__c == NULL &amp;&amp;  Program__c == NULL) ||  
(Award__c != NULL &amp;&amp;  Grant__c == NULL &amp;&amp;  Program__c == NULL) || 
(Award__c == NULL &amp;&amp;  Grant__c != NULL &amp;&amp;  Program__c == NULL) ||
(Award__c == NULL &amp;&amp;  Grant__c == NULL &amp;&amp;  Program__c != NULL), false, true)</errorConditionFormula>
        <errorMessage>At a time only one of these fields can be populated: Award, Grant or Program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartDate_Validation_in_Site_Visit_Dates</fullName>
        <active>false</active>
        <description>Start Date Validation in Site Visit Dates</description>
        <errorConditionFormula>IF((RecordType.Name ==&apos;Site Visit&apos; &amp;&amp; Award__c != null &amp;&amp; TEXT(Award__r.AwardType__c) == &apos;New&apos;) &amp;&amp; OR(DateValue(SiteVisitDateTime__c) &lt; TODAY(),DateValue(SiteVisitDateTime__c) &lt; Award__r.BudgetPeriod__r.BudgetStartDate__c ,DateValue(SiteVisitDateTime__c) &gt; Award__r.BudgetPeriod__r.BudgetEndDate__c) ,true,false)</errorConditionFormula>
        <errorMessage>Site Visit Start Date can not be before today and should be in between Budget Period Start Date and Budget Period End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WithheldMoney</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(WillAgencywithholdFundsifnotSubmit__c,&apos;Yes&apos;) &amp;&amp; ISBLANK(AmountWithheld__c)</errorConditionFormula>
        <errorMessage>Please provide the percentage of amount withheld.</errorMessage>
    </validationRules>
</CustomObject>
