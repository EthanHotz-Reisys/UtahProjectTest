    <validationRules>
        <fullName>Due_date_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(AND(or(ISCHANGED( EndDate__c ),isnew()), EndDate__c &lt; TODAY()), true, false)</errorConditionFormula>
        <errorMessage>Due Date cannot be earlier than Todayâ€™s Date</errorMessage>
    </validationRules>
    <validationRules>
